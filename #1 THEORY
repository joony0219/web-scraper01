# variable
a_string = "like this"
a_number = 3
a_float = 3.12
a_boolean = True
a_none = None

print(type(a_string))
print(type(a_number))
print(type(a_float))
print(type(a_boolean))
print(type(a_none))

# snake case = a_b_c
# python rule

# sequence에는 list와 tuple이 있다.
days = ["Mon", "Tue", "Wed", "Thur", "Fri"]
# List is mutable sequence: 수정 가능하다.
